# Session secret for webapp (generate a random string for production)
SESSION_SECRET=change-this-in-production

# Logto OAuth Configuration
LOGTO_CLIENT_ID=your-logto-client-id
LOGTO_CLIENT_SECRET=your-logto-client-secret
LOGTO_AUTH_URL=http://localhost:3001/oidc/auth
LOGTO_TOKEN_URL=http://logto:3001/oidc/token
LOGTO_USERINFO_URL=http://logto:3001/oidc/me
LOGTO_CALLBACK_URL=http://localhost:3000/auth/logto/callback

# Casdoor OAuth Configuration
CASDOOR_CLIENT_ID=your-casdoor-client-id
CASDOOR_CLIENT_SECRET=your-casdoor-client-secret
CASDOOR_AUTH_URL=http://localhost:8000/login/oauth/authorize
CASDOOR_TOKEN_URL=http://casdoor:8000/api/login/oauth/access_token
CASDOOR_USERINFO_URL=http://casdoor:8000/api/userinfo
CASDOOR_CALLBACK_URL=http://localhost:3000/auth/casdoor/callback
